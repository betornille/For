//Ejemplo for
#include<stdio.h>
int main(){
    float NOM=0, Sal=0;
    int i;

    for(i=1; i<=15; i++){
       printf("\nEl indice i es: %d\n", i);
       printf("Ingrese el salario del profesor: ");
       scanf("%f", &Sal);
       NOM=NOM+Sal;
       i=i+1;
    }
    printf("Resultado es: %f", &NOM);
    return 0;
}
//printf("\nEl indice i es: %d\n", i);
